{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@constants/*": ["constants/*"],
			"@hooks/*": ["hooks/*"],
			"@utils/*": ["utils/*"],
			"@config/*": ["config/*"],
			"@pages/*": ["pages/*"]
		}
	},
	"include": ["src", "hooks", "utils", "constants", "config", "pages"],
	"exclude": ["node_modules"]
}
